//! { "cases": [ {
//!     "name": "default",
//!     "input": [
//!         {
//!             "entry": "default",
//!             "calldata": [
//!                 "42"
//!             ],
//!             "storage": [
//!                 "99", "100", "101"
//!             ]
//!         }
//!     ],
//!     "expected": [
//!         "226"
//!     ]
//! } ] }

struct Data {
    inner: Inner,
}

struct Inner {
    value: u256,
    next: u256,
    last: u256,
}

contract Test {
    data: Data;

    const TEST: u8 = 42;

    pub fn default(mut self, argument: u8) -> u8 {
        self.data.inner.next += argument as u256;

        argument + Self::TEST + (self.data.inner.next as u8)
    }
}
