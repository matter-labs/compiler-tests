//! { "cases": [ {
//!     "name": "default",
//!     "input": [
//!         {
//!             "entry": "default",
//!             "calldata": [
//!                 "42"
//!             ]
//!         }
//!     ],
//!     "expected": [
//!         "1024"
//!     ]
//! } ] }

contract Test {
    a: u256;
    b: u256;

    pub fn default(mut self, value: u248) -> u248 {
        self.a = 5 as u256;
        self.b = 11 as u256;

        (self.a as u248) + self.quadruple(self.triple(self.double(value))) + (self.b as u248)
    }

    fn double(self, value: u248) -> u248 {
        value * 2
    }

    fn triple(self, value: u248) -> u248 {
        3 * value
    }

    fn quadruple(self, value: u248) -> u248 {
        value * 4
    }
}
