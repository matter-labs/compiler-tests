//! { "cases": [ {
//!     "entry": "false_false",
//!     "expected": 42
//! }, {
//!     "entry": "false_true",
//!     "expected": 42
//! }, {
//!     "entry": "true_false",
//!     "expected": 42
//! }, {
//!     "entry": "true_true",
//!     "expected": 25
//! } ] }

contract Test {
    data: u256;

    pub fn false_false(self) -> u64 {
        Self {
            data: 0 as u256,
        }.default(false, false, 25) as u64
    }

    pub fn false_true(self) -> u64 {
        Self {
            data: 0 as u256,
        }.default(false, true, 25) as u64
    }

    pub fn true_false(self) -> u64 {
        Self {
            data: 0 as u256,
        }.default(true, false, 25) as u64
    }

    pub fn true_true(self) -> u64 {
        Self {
            data: 0 as u256,
        }.default(true, true, 25) as u64
    }

    fn default(mut self, gate_1: bool, gate_2: bool, value: u8) -> u8 {
        self.data = 42 as u256;

        if gate_1 {
            if gate_2 {
                self.data = value as u256;
            }
        }

        self.data as u8
    }
}
