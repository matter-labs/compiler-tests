//!     ],
//!     "expected": [
//!         "126"
//!     ]
//! } ] }

const SIZE: u8 = 4;

contract Test {
    data: [[u8; SIZE]; SIZE];

    const TEST: u8 = 42;

    pub fn default(mut self, argument: u8) -> u8 {
        self.data[3][3] = argument;

        argument + Self::TEST + self.data[3][3]
    }
}
