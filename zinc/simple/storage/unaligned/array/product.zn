//! { "cases": [ {
//!     "entry": "entry",
//!     "engines": ["zkevm"], "expected": 15000
//! } ] }

const SIZE: u8 = 4;

contract Test {
    data: [u16; SIZE];

    pub fn entry(self) -> u64 {
        Self {
            data: [0 as u16; SIZE],
        }.default() as u64
    }

    fn default(mut self) -> u16 {
        self.data[0] = 20 as u16;
        self.data[1] = 15 as u16;
        self.data[2] = 10 as u16;
        self.data[3] = 5 as u16;

        let mut product: u16 = 1;
        for i in 0..SIZE {
            product *= self.data[i];
        }
        product
    }
}
