//! { "cases": [ {
//!     "entry": "entry",
//!     "engines": ["zkevm"], "expected": 126
//! } ] }

struct Data {
    value: u8,
}

contract Test {
    data: Data;

    const TEST: u8 = 42;

    pub fn entry(self) -> u64 {
        Self {
            data: Data { value: 0 },
        }.default(42) as u64
    }

    fn default(mut self, argument: u8) -> u8 {
        self.data.value += argument;

        argument + Self::TEST + self.data.value
    }
}
