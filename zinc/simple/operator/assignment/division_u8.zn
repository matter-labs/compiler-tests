//! { "cases": [ {
//!     "entry": "zero_by_zero",
//!     "engines": ["zkevm"], "expected": "error"
//! }, {
//!     "entry": "ordinar_by_zero",
//!     "engines": ["zkevm"], "expected": "error"
//! }, {
//!     "entry": "max_by_zero",
//!     "engines": ["zkevm"], "expected": "error"
//! }, {
//!     "entry": "ordinar",
//!     "expected": 6
//! }, {
//!     "entry": "ordinar_max_remainder",
//!     "expected": 6
//! }, {
//!     "entry": "ordinar_next",
//!     "expected": 7
//! }, {
//!     "entry": "to_max",
//!     "expected": 255
//! }, {
//!     "entry": "to_one",
//!     "expected": 1
//! } ] }

fn zero_by_zero() -> u64 {
    main(0, 0) as u64
}

fn ordinar_by_zero() -> u64 {
    main(42, 0) as u64
}

fn max_by_zero() -> u64 {
    main(255, 0) as u64
}

fn ordinar() -> u64 {
    main(42, 7) as u64
}

fn ordinar_max_remainder() -> u64 {
    main(48, 7) as u64
}

fn ordinar_next() -> u64 {
    main(49, 7) as u64
}

fn to_max() -> u64 {
    main(255, 1) as u64
}

fn to_one() -> u64 {
    main(255, 255) as u64
}

fn main(a: u8, b: u8) -> u8 {
    let mut c = a;
    c /= b;
    c
}
