fn zero_zero() -> u64 {
    if main(0, 0) { 1 } else { 0 }
}

fn ordinar_false_lesser() -> u64 {
    if main(-25, 42) { 1 } else { 0 }
}

fn ordinar_true_bigger() -> u64 {
    if main(42, -25) { 1 } else { 0 }
}

fn ordinar_false_negative() -> u64 {
    if main(-42, -42) { 1 } else { 0 }
}

fn ordinar_false_positive() -> u64 {
    if main(42, 42) { 1 } else { 0 }
}

fn min_min() -> u64 {
    if main(-128, -128) { 1 } else { 0 }
}

fn max_max() -> u64 {
    if main(127, 127) { 1 } else { 0 }
}

fn main(a: i8, b: i8) -> bool {
    a > b
}
