//!     "entry": "fifth",
//!     "expected": 120
//! }, {
//!     "entry": "twelfth",
//!     "expected": 479001600
//! } ] }

fn zero() -> u64 {
    fact(0) as u64
}

fn fifth() -> u64 {
    fact(5) as u64
}

fn twelfth() -> u64 {
    fact(12) as u64
}

fn fact(n: u8) -> u64 {
    if n == 0 {
        return 1;
    }

    fact(n - 1) * (n as u64)
}
