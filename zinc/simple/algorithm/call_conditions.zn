//! { "cases": [ {
//!     "entry": "first",
//!     "expected": 42
//! }, {
//!     "entry": "second",
//!     "expected": 42
//! } ] }

fn first() -> u64 {
    (main(84, 2, 43) + main(23, 23, 42) - main(22, 2, 3)) as u64
}

fn second() -> u64 {
    (main(15, 10, 43) + main(30, 10, 42) - main(3, 1, 3)) as u64
}

fn main(a: u256, b: u256, c: u256) -> u256 {
    if c > 42 {
        return a + b;
    }
    if c == 42 {
        return a - b;
    }
    a * b
}
