//! { "cases": [ {
//!     "entry": "entry",
//!     "expected": 42
//! } ] }

struct Inner {
    value: u8
}

struct Data {
    inner: Inner,
}

fn entry() -> u64 {
    let data = Data {
        inner: Inner {
            value: 168,
        }
    };

    main(data, 4).inner.value as u64
}

fn main(mut data: Data, divider: u8) -> Data {
    data.inner.value /= divider;
    data
}
